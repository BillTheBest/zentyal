package <% $package %>;
use strict;
use warnings;

% if (defined($parent)) {
use base '<% $parent %>';
% } else {
use base 'EBox::Types::Abstract';
% }

use EBox::Exceptions::MissingArgument;

sub new
{
    my $class = shift;
    my %opts = @_;
    my $self = $class->SUPER::new(@_);
    bless($self, $class);

    return $self;
}

1;
