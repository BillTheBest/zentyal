Dir {
  ArchiveDir "ARCHIVE_DIR";
};

TreeDefault {
  Directory "pool/";
};

BinDirectory "pool/main" {
  Packages "dists/lucid/main/binary-ARCH/Packages";
  BinOverride "INDICES/override.lucid.main";
  ExtraOverride "INDICES/override.lucid.extra.main";
};

BinDirectory "pool/restricted" {
 Packages "dists/lucid/restricted/binary-ARCH/Packages";
 BinOverride "INDICES/override.lucid.restricted";
};

Default {
  Packages {
    Extensions ".deb";
    Compress ". gzip";
  };
};

Contents {
  Compress "gzip";
};
