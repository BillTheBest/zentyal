msgstr "eBox-användaren har inte ställts in i konfigurationsfilen."

#: src/EBox/Exceptions/DataNotFound.pm:37
#, perl-brace-format
msgid "{data} {value} does not exist."
msgstr "{data} {value} finns inte."

#: src/EBox/Exceptions/DataExists.pm:38
#, perl-brace-format
