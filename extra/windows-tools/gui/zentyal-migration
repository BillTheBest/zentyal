#!/usr/bin/python

import gtk

class MigrationApp(object):
    def __init__(self):
        builder = gtk.Builder()
        builder.add_from_file("migration.xml")
        builder.connect_signals({ "on_mainWindow_destroy" : gtk.main_quit })
        self.window = builder.get_object("mainWindow")
        self.window.show()

if __name__ == "__main__":
    app = MigrationApp()
    gtk.main()
